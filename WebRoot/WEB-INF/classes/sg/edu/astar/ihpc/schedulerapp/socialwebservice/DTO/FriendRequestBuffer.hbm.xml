<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="sg.edu.astar.ihpc.schedulerapp.socialwebservice.DTO.FriendRequestBuffer" table="FRIEND_REQUEST_BUFFER">
		<id name="id" column="ID" type="integer">
			<generator class="identity"></generator>
		</id>
		<property name="sendRequestUserEmail" column="SEND_REQUEST_USER_EMAIL" type="string" length="500" not-null="true">
		</property>
		<property name="receiveRequestUserEmail" column="RECEIVE_REQUEST_USER_EMAIL" type="string" length="500" not-null="true">
		</property>
	</class>
</hibernate-mapping>